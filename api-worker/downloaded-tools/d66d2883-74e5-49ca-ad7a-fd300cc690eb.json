{
  "name": "@cnbcool/mcp-server",
  "description": "@cnbcool/mcp-server MCP Server",
  "url": "@cnbcool/mcp-server",
  "protocol_version": "MCP/1.0",
  "capabilities": [
    {
      "name": "list-groups",
      "type": "tool",
      "description": "获取当前用户拥有权限的顶层组织列表"
    },
    {
      "name": "list-sub-groups",
      "type": "tool",
      "description": "查询当前用户在指定组织下拥有指定权限的子组织列表"
    },
    {
      "name": "get-group",
      "type": "tool",
      "description": "获取指定组织信息"
    },
    {
      "name": "create-group",
      "type": "tool",
      "description": "创建新组织"
    },
    {
      "name": "list-repositories",
      "type": "tool",
      "description": "获取当前用户拥有指定权限及其以上权限的仓库"
    },
    {
      "name": "list-group-repositories",
      "type": "tool",
      "description": "获取分组里当前用户有权限的仓库"
    },
    {
      "name": "get-repository",
      "type": "tool",
      "description": "获取指定仓库信息"
    },
    {
      "name": "create-repository",
      "type": "tool",
      "description": "创建仓库"
    },
    {
      "name": "list-issues",
      "type": "tool",
      "description": "查询仓库的 Issues"
    },
    {
      "name": "get-issue",
      "type": "tool",
      "description": "获取指定 Issue 信息"
    },
    {
      "name": "create-issue",
      "type": "tool",
      "description": "创建一个 Issue"
    },
    {
      "name": "list-workspace",
      "type": "tool",
      "description": "获取我的云原生开发环境列表"
    },
    {
      "name": "delete-workspace",
      "type": "tool",
      "description": "删除我的云原生开发环境"
    }
  ],
  "installation": {
    "command": "npx",
    "args": [
      "-y",
      "@cnbcool/mcp-server"
    ],
    "env": {}
  },
